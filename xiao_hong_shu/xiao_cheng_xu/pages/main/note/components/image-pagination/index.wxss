.pagination-container {
    align-items: center;
    display: flex;
    height: 50rpx;
    justify-content: center;
    width: 100%;
}

.normal-dot {
    background: #e6e6e6;
    border-radius: 50%;
    height: 10rpx;
    margin: 0 4rpx;
    width: 10rpx;
}

.active-dot {
    background: #ff2741!important;
}

.small-dot {
    background: #e6e6e6;
    border-radius: 50%;
    height: 6rpx;
    margin: 0 4rpx;
    width: 6rpx;
}