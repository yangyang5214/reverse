.submit-container {
    -webkit-tap-highlight-color: none;
    background-color: none;
    background: none;
    border-radius: 0;
    color: inherit;
    display: flex;
    font-size: inherit;
    line-height: inherit;
    padding: 0;
    text-align: inherit;
}

.submit-container:after {
    border: none;
}

.submit-container .button-hover {
    background-color: none;
}