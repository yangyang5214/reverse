.page-container {
    opacity: 0;
    width: 100%;
}

.page-container.ready {
    opacity: 1;
}

.page-content {
    position: relative;
}

.navigation-mini-bar {
    align-items: center;
    display: flex;
    height: 64rpx;
    left: 28rpx;
    top: 52rpx;
}

.navigation-bar,.navigation-mini-bar {
    position: fixed;
    width: 100%;
    z-index: 10010;
}

.navigation-bar {
    background: #fff;
    color: #333;
    top: 0;
}

.navigation-bar.no-bg {
    background: none;
}

.navigation-bar .navigation-bar-inner {
    align-items: center;
    display: flex;
    height: 88rpx;
    line-height: 88rpx;
    position: relative;
    width: 100%;
}

.navigation-bar .navigation-bar-side {
    box-sizing: border-box;
    height: 64rpx;
    margin: 8rpx;
    position: relative;
    width: 174rpx;
}

.navigation-bar .navigation-bar-side.left-side {
    margin-left: 30rpx;
}

.navigation-bar .navigation-bar-side.right-side {
    margin-right: 30rpx;
}

.navigation-bar .back-defeat {
    border: .5px solid #e9e9e9;
    border-radius: 30rpx;
}

.navigation-bar .back-defeat.no-border {
    border: none;
}

.navigation-bar .left-side-inner {
    display: flex;
    max-width: 173rpx;
    position: absolute;
}

.navigation-bar .navigation-bar-center {
    flex: 1;
    font-size: 32rpx;
    font-weight: 500;
    line-height: 88rpx;
    overflow: hidden;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.navigation-gap {
    background: #e9e9e9;
    height: 36rpx;
    left: 49%;
    position: absolute;
    top: 15rpx;
    width: .5px;
}

.back-icon-container-defeat {
    justify-content: center;
    padding-left: 5rpx;
}

.navigation-icon-container {
    align-items: center;
    display: inline-flex;
    height: 100%;
    justify-content: center;
    padding-right: 5rpx;
    width: 72rpx;
}

.navigation-icon-container .navigation-back-icon,.navigation-icon-container .navigation-home-icon {
    height: 72rpx;
    width: 72rpx;
}