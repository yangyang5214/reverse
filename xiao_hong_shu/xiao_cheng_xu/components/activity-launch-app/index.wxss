.launch-app-container {
    bottom: 150rpx;
    position: fixed;
    right: 30rpx;
    z-index: 9999;
}

.launch-app {
    background: none;
    border-radius: 50%;
    display: flex;
    line-height: 112rpx;
    padding: 0;
}

.launch-app .logo {
    height: 112rpx;
    width: 112rpx;
}

.launch-app-container button:after {
    border: none;
    border-radius: 50%;
}